  mysql:
    image: {{ @('services.mysql.image') }}
    labels:
      - traefik.enable=false
    environment: {{ to_nice_yaml(deep_merge([
        @('services.mysql.environment'),
        @('services.mysql.environment_secrets')
      ]), 2, 6) | raw }}
    networks:
      - private
    ports:
      - 3306
