  mysql:
    image: mysql:{{ @('services.mysql.version') }}
    labels:
      - traefik.enable=false
    environment: {{ to_nice_yaml(deep_merge([
        @('services.mysql.environment'),
        @('services.mysql.environment_secrets')
      ]), 2, 6) | raw }}
    networks:
      - private
{% if @('app.build') == 'develop' %}
    ports:
      - 3306
{% endif %}
