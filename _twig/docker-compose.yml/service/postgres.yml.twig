  postgres:
    image: postgres:{{ @('services.postgres.version') }}
    labels:
      - traefik.enable=false
    environment: {{ to_nice_yaml(deep_merge([
        @('services.postgres.environment'),
        @('services.postgres.environment_secrets')
      ]), 2, 6) | raw }}
    networks:
      - private
    ports:
        - 5432
